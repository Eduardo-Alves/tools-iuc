<?xml version="1.0"?>
<tool id="hmmer.alimask" name="alimask" version="@WRAPPER_VERSION@.0">
  <description>appending modelmask line to a multiple sequence alignments</description>
  <macros>
    <import>macros.xml</import>
  </macros>
  <expand macro="requirements"/>
  <expand macro="stdio"/>
  <command><![CDATA[
alimask

@FORMAT_SELECTOR@
@MCSS@
@ARSWS@
@SEED@

> $output
      ]]></command>
  <inputs>
      <param name="msafile" type="data"
          label="MSA File" />
      <param name="postmsafile" type="data"
          label="Post MSA File" />
      <!--[> todo use Galaxy features like data libraries/data tables/??? <]-->
      <!--<param name="seqdb" type="data" format="fasta" label="Sequence Database" />-->
      <expand macro="format_selector" />
      <expand macro="mcss" />
      <expand macro="arsws" />
      <expand macro="seed" />
  </inputs>
  <outputs>
    <data format="txt" name="output" label="Output MSA"/>
  </outputs>
  <tests>
  </tests>
  <help><![CDATA[
**What it does**

Modifies alignment file to mask column ranges.


@ATTRIBUTION@
]]></help>
    <expand macro="citation" />
</tool>
