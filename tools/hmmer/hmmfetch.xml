<?xml version="1.0"?>
<tool id="hmmer.hmmfetch" name="hmmfetch" version="@WRAPPER_VERSION@.0">
  <description>retrieve profile HMM(s) from a file</description>
  <macros>
    <import>macros.xml</import>
  </macros>
  <expand macro="requirements"/>
  <expand macro="stdio"/>
  <command><![CDATA[
hmmfetch -f $hmmfile $keyfile > $output;
      ]]></command>
  <inputs>
    <param name="hmmfile" type="data" label="HMM model"/>
    <param name="keyfile" type="data" label="List of HMM names to extract"/>
  </inputs>
  <outputs>
    <data format="txt" name="output" label="Selected HMM Models from $hmmfile.name">
    </data>
  </outputs>
  <tests>
    <test>
      <param name="hmmfile" value="fn3.hmm"/>
      <param name="keyfile" value="fn3.keys"/>
      <output name="output" file="fn3.hmm" lines_diff="4/>
    </test>
  </tests>
  <help><![CDATA[
**What it does**

Extract a profile HMM by name or accession from an HMM database.

@ATTRIBUTION@
]]></help>
  <expand macro="citation"/>
</tool>
