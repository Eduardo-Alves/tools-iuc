<tool force_history_refresh="false" hidden="false" id="seqtk_fqchk_1.0" name="seqtk_fqchk" version="1.0" workflow_compatible="true">
    <description>fastq QC (base/quality summary)</description>
  <stdio>
    <exit_code level="fatal" range="1:"/>
  </stdio>
  <command><![CDATA[seqtk fqchk
-q $q
> $default]]></command>
  <inputs>
    <param name="q" type="integer" value="20" label="quality values (-q)" help="use 0 to get the distribution of all quality values"/>
    <param name="in_fq" type="data" format="fastq" label="Input fastq fle"/>
  </inputs>
  <outputs>
    <data format="tabular" hidden="false" name="default"/>
  </outputs>
  <help><![CDATA[TODO]]></help>
</tool>
