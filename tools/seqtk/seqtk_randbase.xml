<tool force_history_refresh="false" hidden="false" id="seqtk_randbase_1.0" name="seqtk_randbase" version="1.0" workflow_compatible="true">
  <description>choose a random base from hets</description>
  <stdio>
    <exit_code level="fatal" range="1:"/>
  </stdio>
  <command><![CDATA[seqtk randbase
      $in_fa
> $default]]></command>
