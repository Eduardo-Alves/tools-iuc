<tool force_history_refresh="false" hidden="false" id="seqtk_sample_1.0" name="seqtk_sample" version="1.0" workflow_compatible="true">
  <description></description>
  <stdio>
    <exit_code level="fatal" range="1:"/>
  </stdio>
  <command><![CDATA[seqtk sample $x2 -s $s
      $in_file
      $subsample_size
> $default]]></command>
  <inputs>
      <param name="in_file" type="data" format="fasta,fastq" label="Input fasta/q file"/>
    <param label="RNG seed (-s)" name="s" type="integer" value="11"/>
    <param label="Subsample (decimal fraction or number)" name="subsample_size" type="integer" value="100"/>
    <param checked="false" label="2-pass mode: twice as slow but with much reduced memory (-2)" name="x2" type="boolean" truevalue="-2"/>
  </inputs>
  <outputs>
    <data format_source="in_file" hidden="false" name="default"/>
  </outputs>
  <help><![CDATA[TODO]]></help>
</tool>
