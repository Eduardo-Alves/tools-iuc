<tool force_history_refresh="false" hidden="false" id="seqtk_hety_1.0" name="seqtk_hety" version="1.0" workflow_compatible="true">
  <description>regional heterozygosity</description>
  <stdio>
    <exit_code level="fatal" range="1:"/>
  </stdio>
  <command><![CDATA[seqtk hety -w $w
-t $t
$m
> $default]]></command>
  <inputs>
    <param label="window size (-w)" name="w" type="integer" value="50000"/>
    <param label="# start positions in a window (-t)"  name="t" type="integer" value="5"/>
    <param checked="false" label="treat lowercases as masked (-m)" name="m" type="boolean" truevalue="-m"/>
  </inputs>
  <outputs>
    <data format="txt" hidden="false" name="default"/>
  </outputs>
  <help><![CDATA[TODO]]></help>
</tool>
