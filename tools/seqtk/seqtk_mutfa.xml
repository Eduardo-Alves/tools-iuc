<tool force_history_refresh="false" hidden="false" id="seqtk_mutfa_1.0" name="seqtk_mutfa" version="1.0" workflow_compatible="true">
  <description>point mutate FASTA at specified positions</description>
  <stdio>
    <exit_code level="fatal" range="1:"/>
  </stdio>
  <command><![CDATA[seqtk mutfa
      $in_fa $in_snp
> $default]]></command>
  <inputs>
    <param name="in_fa" type="data" format="fasta" label="Input fasta file"/>
    <param name="in_snp" type="data" format="vcf" label="Input SNP file"/>
  </inputs>
  <outputs>
    <data format="fasta" hidden="false" name="default"/>
  </outputs>
  <help><![CDATA[TODO]]></help>
</tool>
